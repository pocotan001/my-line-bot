{
  "compilerOptions": {
    "allowJs": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "jsx": "react",
    "lib": ["dom", "es2017"],
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitUseStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "pretty": true,
    "strictNullChecks": true,
    "target": "es2015"
  }
}
